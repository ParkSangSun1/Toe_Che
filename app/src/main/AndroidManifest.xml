<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.children.toyexchange">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ToyExchange">
        <activity android:name=".presentation.view.main.mainfragment.search.SearchRecordActivity"></activity>
        <activity android:name=".presentation.view.main.MainActivity">

        </activity>
        <activity android:name=".presentation.view.signIn.PhoneAuthActivity"></activity>
        <activity android:name=".presentation.view.signIn.SignInActivity"></activity>
        <activity android:name=".presentation.view.myToyUpload.ToyUploadActivity" />

        <activity android:name=".presentation.view.splash.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>